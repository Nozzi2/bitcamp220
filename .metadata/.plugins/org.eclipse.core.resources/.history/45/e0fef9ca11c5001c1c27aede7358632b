package inheritance;

class AA {
	protected int a = 3;
	
	public void disp() {
		a += 5;
		System.out.println("AA : "+a+" ");
	}
}

//---------------------------------

class BB extends AA {
	protected int a = 8;
	
	public void disp() {
		a += 5; //this.a = this.a+5;
		System.out.println("BB : "+a+" ");
	}
}

//---------------------------------
public class TestMain2 {

	public static void main(String[] args) {
		BB aa = new BB();
		aa.disp(); //BB : 13//
		System.out.println("aa : "+aa.a); //13
		
		AA bb = new BB();
		bb.disp(); //BB : 13
		System.out.println("bb : "+bb.a); //
		
		AA cc = new AA();
		cc.disp(); //AA : 8
		System.out.println("cc : "+cc.a); //8
		
		//자식 = (자식)부모
//		BB dd = (BB)bb;
		BB dd = (BB)cc;
		dd.disp(); //BB : 13
		System.out.println("dd : "+dd.a); //
	}

}
